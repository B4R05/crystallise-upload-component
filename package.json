{
  "name": "uploader",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.24",
    "@types/node": "^12.20.27",
    "@types/react": "^17.0.27",
    "@types/react-dom": "^17.0.9",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "react-semantic-ui-range": "^0.7.1",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.4",
    "typescript": "^4.4.3",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "react-app"
    ],
    "rules": {
      "no-var": "error",
      "eqeqeq": [
        "error",
        "always"
      ],
      "import/no-anonymous-default-export": [
        "error",
        {
          "allowArrowFunction": true
        }
      ],
      "curly": 0,
      "block-spacing": "warn",
      "keyword-spacing": [
        "warn",
        {
          "before": true,
          "after": true
        }
      ],
      "no-whitespace-before-property": "warn",
      "no-multi-spaces": "warn",
      "space-in-parens": [
        "warn",
        "never"
      ],
      "space-infix-ops": "warn",
      "space-unary-ops": [
        "warn",
        {
          "words": true,
          "nonwords": false
        }
      ],
      "key-spacing": [
        "warn",
        {
          "singleLine": {
            "beforeColon": false,
            "afterColon": true
          },
          "multiLine": {
            "beforeColon": false,
            "afterColon": true
          }
        }
      ],
      "array-bracket-spacing": [
        "warn",
        "never"
      ],
      "arrow-spacing": [
        "warn",
        {
          "before": true,
          "after": true
        }
      ],
      "comma-spacing": [
        "warn",
        {
          "before": false,
          "after": true
        }
      ],
      "computed-property-spacing": [
        "warn",
        "never"
      ],
      "func-call-spacing": [
        "warn",
        "never"
      ],
      "object-curly-spacing": [
        "warn",
        "always"
      ],
      "space-before-function-paren": [
        "warn",
        {
          "anonymous": "never",
          "named": "never",
          "asyncArrow": "always"
        }
      ],
      "template-curly-spacing": "warn",
      "max-len": [
        "warn",
        {
          "code": 120,
          "ignoreUrls": true,
          "ignoreStrings": true,
          "ignoreTemplateLiterals": true
        }
      ],
      "no-console": [
        "warn",
        {
          "allow": [
            "warn",
            "error"
          ]
        }
      ],
      "indent": [
        "warn",
        2,
        {
          "SwitchCase": 1,
          "CallExpression": {
            "arguments": "first"
          }
        }
      ],
      "semi": [
        "warn",
        "always"
      ],
      "comma-dangle": [
        "warn",
        {
          "arrays": "never",
          "objects": "never",
          "imports": "never",
          "exports": "never",
          "functions": "never"
        }
      ],
      "quotes": [
        "warn",
        "single"
      ],
      "spaced-comment": [
        "error",
        "always",
        {
          "exceptions": [
            "-",
            "+"
          ]
        }
      ],
      "react-hooks/exhaustive-deps": 0
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
